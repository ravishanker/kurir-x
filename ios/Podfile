target 'Kurir' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  source 'https://github.com/CocoaPods/Specs.git'
  platform :ios, '9.0'
  use_frameworks!
  
  pod 'Mobilex', :path => '../Mobilex'
  pod 'Crazybean', :git => 'https://crazybeanstudio@bitbucket.org/crazybeanstudio/crazybean-ios.git', :tag => '1.0.0'
  pod 'Swinject'
  pod 'Firebase/Core'
  pod 'Firebase/Firestore'
  pod 'Firebase/Messaging'
end
